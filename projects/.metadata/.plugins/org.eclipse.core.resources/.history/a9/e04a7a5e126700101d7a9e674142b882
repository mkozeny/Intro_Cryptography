package main;

public class Complex {

	private double re;

	private double im;

	public Complex() {
		super();
	}

	public Complex(double re, double im) {
		super();
		this.re = re;
		this.im = im;
	}

	public double getRe() {
		return re;
	}

	
	public void setRe(double re) {
		this.re = re;
	}
	
	public double timesItself()
	{
		return re*re + im*im;
	}
	
	public boolean isZero()
	{
		return this.re==0 && this.im == 0;
	}
	
	public double getIm() {
		return im;
	}

	public void setIm(double im) {
		this.im = im;
	}

}
