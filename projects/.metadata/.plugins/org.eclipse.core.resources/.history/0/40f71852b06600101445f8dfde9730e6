package main;

public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Complex c1 = new Complex(0, 1/Math.sqrt(2.0));
		Complex c2 = new Complex(1/Math.sqrt(2.0), 0);
		Vector v = new Vector(c1, c2);
		System.out.println("Length of vector: "+v.length());
		System.out.println("Is unit vector: "+v.isUnitLength());
	}

}
