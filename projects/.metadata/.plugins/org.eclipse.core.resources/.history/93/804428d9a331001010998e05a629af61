package eukleidian;

public class Equation {

	private Integer k;
	
	private Integer leftOprand;
	
	private Integer l;
	
	private Integer rightOperand;
	
	private Integer remainder;
	
	

	
	public Equation(Integer k,Integer leftOprand, Integer l, Integer rightOperand,
			Integer remainder) {
		super();
		this.k = k;
		this.leftOprand = leftOprand;
		this.l = l;
		this.rightOperand = rightOperand;
		this.remainder = remainder;
	}

	public Integer getK() {
		return k;
	}

	public void setK(Integer k) {
		this.k = k;
	}

	public Integer getLeftOprand() {
		return leftOprand;
	}

	public void setLeftOprand(Integer leftOprand) {
		this.leftOprand = leftOprand;
	}
	
	

	public Integer getL() {
		return l;
	}

	public void setL(Integer l) {
		this.l = l;
	}

	public Integer getRightOperand() {
		return rightOperand;
	}

	public void setRightOperand(Integer rightOperand) {
		this.rightOperand = rightOperand;
	}

	public Integer getRemainder() {
		return remainder;
	}

	public void setRemainder(Integer remainder) {
		this.remainder = remainder;
	}
	
	
}
